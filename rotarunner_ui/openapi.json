  "components": {
    "schemas": {
      "ActivityDisplay": {
        "properties": {
          "activity_id": {
            "type": "string",
            "title": "Activity Id"
          },
          "name": {
            "type": "string",
            "title": "Name"
          },
          "timeslots": {
            "items": {
              "$ref": "#/components/schemas/ActivityTimeslot"
            },
            "type": "array",
            "title": "Timeslots"
          },
          "location": {
            "anyOf": [
              {
                "$ref": "#/components/schemas/LabelledUUID"
              },
              {
                "type": "null"
              }
            ]
          }
        },
        "type": "object",
        "required": [
          "activity_id",
          "name",
          "timeslots",
          "location"
        ],
        "title": "ActivityDisplay"
      },
      "ActivityResponse": {
        "properties": {
          "data": {
            "additionalProperties": {
              "items": {
                "$ref": "#/components/schemas/ActivityDisplay"
              },
              "type": "array"
            },
            "propertyNames": {
              "format": "date"
            },
            "type": "object",
            "title": "Data"
          },
          "toasts": {
            "items": {
              "$ref": "#/components/schemas/Toast"
            },
            "type": "array",
            "title": "Toasts"
          }
        },
        "type": "object",
        "required": [
          "data",
          "toasts"
        ],
        "title": "ActivityResponse"
      },
      "ActivityTimeslot": {
        "properties": {
          "start": {
            "type": "string",
            "format": "date-time",
            "title": "Start"
          },
          "finish": {
            "type": "string",
            "format": "date-time",
            "title": "Finish"
          },
          "staff_assignments": {
            "items": {
              "$ref": "#/components/schemas/StaffAssignmentDisplay"
            },
            "type": "array",
            "title": "Staff Assignments",
            "default": []
          }
        },
        "type": "object",
        "required": [
          "start",
          "finish"
        ],
        "title": "ActivityTimeslot"
      },
      "DateRange": {
        "properties": {
          "start": {
            "type": "string",
            "format": "date",
            "title": "Start"
          },
          "finish": {
            "type": "string",
            "format": "date",
            "title": "Finish"
          }
        },
        "type": "object",
        "required": [
          "start",
          "finish"
        ],
        "title": "DateRange"
      },
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "LabelledUUID": {
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid",
            "title": "Id"
          },
          "name": {
            "type": "string",
            "title": "Name"
          }
        },
        "type": "object",
        "required": [
          "id",
          "name"
        ],
        "title": "LabelledUUID"
      },
      "LocationGridCell": {
        "properties": {
          "date": {
            "type": "string",
            "format": "date",
            "title": "Date"
          },
          "location": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Location"
          }
        },
        "type": "object",
        "required": [
          "date"
        ],
        "title": "LocationGridCell"
      },
      "MoveActivityInLocationGrid": {
        "properties": {
          "activity_id": {
            "type": "string",
            "title": "Activity Id"
          },
          "from_cell": {
            "$ref": "#/components/schemas/LocationGridCell"
          },
          "to_cell": {
            "$ref": "#/components/schemas/LocationGridCell"
          }
        },
        "type": "object",
        "required": [
          "activity_id",
          "from_cell",
          "to_cell"
        ],
        "title": "MoveActivityInLocationGrid"
      },
      "MoveActivityInStaffGrid": {
        "properties": {
          "activity_id": {
            "type": "string",
            "title": "Activity Id"
          },
          "from_cell": {
            "$ref": "#/components/schemas/StaffGridCell"
          },
          "to_cell": {
            "$ref": "#/components/schemas/StaffGridCell"
          }
        },
        "type": "object",
        "required": [
          "activity_id",
          "from_cell",
          "to_cell"
        ],
        "title": "MoveActivityInStaffGrid"
      },
      "MoveStaffInLocationGrid": {
        "properties": {
          "staffId": {
            "type": "string",
            "title": "Staffid"
          },
          "from_activity": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "From Activity"
          },
          "to_activity": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "To Activity"
          }
        },
        "type": "object",
        "required": [
          "staffId"
        ],
        "title": "MoveStaffInLocationGrid"
      },
      "RotaConfig": {
        "properties": {
          "date_range": {
            "$ref": "#/components/schemas/DateRange"
          },
          "staff": {
            "items": {
              "$ref": "#/components/schemas/LabelledUUID"
            },
            "type": "array",
            "title": "Staff"
          },
          "locations": {
            "items": {
              "$ref": "#/components/schemas/LabelledUUID"
            },
            "type": "array",
            "title": "Locations"
          }
        },
        "type": "object",
        "required": [
          "date_range",
          "staff",
          "locations"
        ],
        "title": "RotaConfig"
      },
      "StaffAssignmentDisplay": {
        "properties": {
          "staff": {
            "$ref": "#/components/schemas/LabelledUUID"
          },
          "start_time": {
            "anyOf": [
              {
                "type": "integer"
              },
              {
                "type": "null"
              }
            ],
            "title": "Start Time"
          },
          "finish_time": {
            "anyOf": [
              {
                "type": "integer"
              },
              {
                "type": "null"
              }
            ],
            "title": "Finish Time"
          }
        },
        "type": "object",
        "required": [
          "staff"
        ],
        "title": "StaffAssignmentDisplay"
      },
      "StaffGridCell": {
        "properties": {
          "date": {
            "type": "string",
            "format": "date",
            "title": "Date"
          },
          "staff": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Staff"
          }
        },
        "type": "object",
        "required": [
          "date"
        ],
        "title": "StaffGridCell"
      },
      "Toast": {
        "properties": {
          "kind": {
            "type": "string",
            "enum": [
              "success",
              "error"
            ],
            "title": "Kind"
          },
          "title": {
            "type": "string",
            "title": "Title"
          },
          "description": {
            "type": "string",
            "title": "Description"
          }
        },
        "type": "object",
        "required": [
          "kind",
          "title",
          "description"
        ],
        "title": "Toast"
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "integer"
                }
              ]
            },
            "type": "array",
            "title": "Location"
          },
          "msg": {
            "type": "string",
            "title": "Message"
          },
          "type": {
            "type": "string",
            "title": "Error Type"
          }
        },
        "type": "object",
        "required": [
          "loc",
          "msg",
          "type"
        ],
        "title": "ValidationError"
      }
    }
  }
}