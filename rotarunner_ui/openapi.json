    "/api/activities_by_date": {
      "get": {
        "summary": "Get Activities Grouped By Date",
        "description": "Get activities grouped by date.",
        "operationId": "getActivitiesByDate",
        "parameters": [
          {
            "name": "start_date",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string",
              "format": "date",
              "default": "1970-01-01",
              "title": "Start Date"
            }
          },
          {
            "name": "finish_date",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string",
              "format": "date",
              "default": "2100-01-01",
              "title": "Finish Date"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "anyOf": [
                  {
                    "type": "array",
                    "items": {
                      "$ref": "#/components/schemas/Toast"
                    }
                  },
                  {
                    "type": "null"
                  }
                ],
                "title": "Toasts"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ActivityResponse"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
